{
  "name": "node-graphql",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^2.18.0",
    "@types/node": "^14.14.32",
    "axios": "^0.21.1",
    "cron-parser": "^3.0.0",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "graphql": "^15.4.0",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.31.0",
    "node-schedule": "^1.3.2",
    "prisma": "^2.18.0",
    "typescript": "^4.2.3",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "cross-env": "^7.0.3"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=develop nodemon --exec ts-node-dev --tree-kill --no-notify --respawn --transpile-only src/index.ts",
    "serve": "cross-env NODE_ENV=production nodemon --exec ts-node --transpile-only server/src/index.ts",
    "admin": "npx dotenv-cli -e .env -- npx prisma studio",
    "clean": "rm -rf dist",
    "build": "yarn -s clean && yarn -s generate && tsc",
    "style": "prettier --write 'src/**/*'",
    "dev:migrate": "prisma migrate dev --preview-feature",
    "generate": "yarn -s generate:prisma",
    "generate:prisma": "prisma generate"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
